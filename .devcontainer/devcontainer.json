{
  "name": "Symfony Auth Methods",
  "dockerComposeFile": "../docker-compose.yml",
  "service": "app",
  "workspaceFolder": "/var/www/html",
  "forwardPorts": [8000],
  "portsAttributes": {
    "8000": {
      "label": "Symfony Application",
      "onAutoForward": "notify",
      "visibility": "public"
    }
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "mikestead.dotenv",
        "bmewburn.vscode-intelephense-client"
      ]
    }
  },
  "remoteUser": "www-data"
}
